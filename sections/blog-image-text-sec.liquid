<div class="blog-image-tetxt">
    <div class="wrapper-in" >
        <div class="image-over-blog-two">
        	<div class="image-over-blog-text">
			 {% if section.settings.left_image %} 
              <img class="img1" src="{{ section.settings.left_image | img_url: "master" }}">
              {% endif %}
        	</div>
          
          <div class="blog-right-content">
            
            {%- if section.settings.preheading != blank -%}
			  	<h4>{{ section.settings.preheading }}</h4>
			  {%- endif -%}
              {%- if section.settings.heading != blank -%}
			  	<h2>{{ section.settings.heading }}</h2>
			  {%- endif -%}
              {%- if section.settings.text2 != blank -%}
			  	{{ section.settings.text2 }}
			  {%- endif -%}
              <a class="btn image-over-blog-read" href="{{ section.settings.url1 }}"><span class="noselect">{{ section.settings.text22 }}</span></a>
            
          </div>
            
        </div>
  </div>
</div>  

    
{% schema %}
{
  "name": "Blog image text ",
  "class": "image_over-s",  
      "settings": [					
		{
          "type": "image_picker",
          "id": "left_image",
          "label": "left_image"
        },
		{
          "type": "image_picker",
          "id": "back_image1",
          "label": "Mobile Back Image"
        },
		{
          "type": "text",
          "id": "preheading",
          "label": "Preheading"
        },
		{
          "type": "text",
          "id": "heading",
          "label": "Heading"
        },
		{
		  "type": "richtext",
          "id": "text2",
          "label": "Paragraph"
        },
		{
          "type": "text",
          "id": "text22",
          "label": "Button"
        },
		{
          "type": "url",
          "id": "url1",
          "label": "Button URL"
        }
      ],
  "presets": [
	{
      "name": "Main Blog image text",
      "category": "Image"
    }
  ]
}

{% endschema %}