<div class="comp-content">
    <div class="wrapper" >
      <div class="comp-all-in" >
        {%- for block in section.blocks -%}
        <div class="comp-one" id="block-{{ block.id }}">
          <div class="comp-image">
            {%- if block.settings.image != blank -%} 
            <a href="{{ block.settings.image_url }}">
              <img class="a-one" src="{{ block.settings.image | img_url: 'master' }}">
            </a>
            {%- endif -%}
          </div>
          <div class="comp-text">
            {%- if block.settings.text11 != blank -%}
              <h2>{{ block.settings.text11 }}</h2>
            {%- endif -%}
            {%- if block.settings.subheading != blank -%}
              <h4 style="color:{{ block.settings.colorsubheading }};'">{{ block.settings.subheading }}</h4>
            {%- endif -%}
            {%- if block.settings.subheadinga != blank -%}
              <h5>{{ block.settings.subheadinga }}</h5>
            {%- endif -%}
            {%- if block.settings.text2 != blank -%}
            <div><a href="{{ block.settings.text_url }} target="_blank" ">{{ block.settings.text2 }}</a></div>
            {%- endif -%}
          </div>
        </div>
      {%- endfor -%}
      </div>
  </div>
</div>
    
    
    
{% schema %}
{
  "name": "Compliance Top",
  "class": "compliance-cls",  
      "settings": [],
  		"blocks": [
     {
       "name": "Doctor",
       "type": "Doctor",
       "settings": [
		{
          "type": "url",
          "id": "image_url",
          "label": "Image url"
        },
        {
          "type": "image_picker",
          "id": "image",
          "label": "Image one"
        },
		{
          "type": "text",
          "id": "text11",
          "label": "Heading"
        },
		{
		  "type": "url",
          "id": "text_url",
          "label": "URL"
        },
		{
		  "type": "text",
          "id": "text2",
          "label": "Paragraph"
        }
      ]
}
   ]
}

{% endschema %}